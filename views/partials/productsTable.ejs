    <div class="d-flex align-items-center justify-content-between">
        <div>
            <h1 class="text-danger"> Vista de productos</h1>
        </div>
        <!-- <div>
            <a class="btn btn-info" href="/" role="button">Volver a la carga</a>
        </div> -->
    </div>
    <% if ( data.productos.length === 0) { %>
    <h2 class="bg-danger rounded text-center m-4">No se encontraron productos</h2>
    <% } else {%>
    <table class="table table-dark">
        <tr class="text-warning">
            <th>Nombre</th>
            <th>Precio</th>
            <th>Imagen</th>
            <th>Detalle</th>

        </tr>
        <% data.productos.forEach(function(producto) { %>
        <tr>
            <td class="align-middle"><%= producto.name %></td>
            <td class="align-middle"><%= `$${producto.price}`  %></td>
            <td class="align-middle"><img style="max-width: 48px" src="<%= producto.photourl %>"
                    alt="<%= producto.name %>">
            <td class="align-middle"> <a class="btn btn-info" href="/productos/<%= producto.id %>" role="button">Ver
                    producto</a>
            </td>

        </tr>
        <% }) %>
    </table>
    <% } %>